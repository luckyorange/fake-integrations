{
  "build": {
    "resources": [
      "readme",
      { 
        "name" : "buddy",
        "options" : {
          "cluster" : { "name" : "api", "zone" : "us-central1-a" },
          "clusterStaging" : { "name" : "api", "zone" : "us-central1-c" }          
        }
      },      
      "docker",
      {
        "name" : "kubernetes",
        "options" : {
          "redis" : true,
          "public" : false
        }
      }
    ]
  },
  "start": {
    "kubernetes": {
      "cluster": "api",
      "project": "lucky-orange-staging",
      "zone": "us-central1-c"
    },
    "services" : [
      {	
        "cluster": "api",	
        "project": "lucky-orange-staging",	
        "zone": "us-central1-c",	
        "name": "service/integrations-databox-redis",	
        "port": 6379	
      }
    ]
  }
}
