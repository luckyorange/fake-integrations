{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
        "@types/luckyorange__*": [
            "node_modules/@luckyorange/type-definitions/*"
        ]
    },
    "typeRoots": [
        "node_modules/@types",
        "node_modules/@luckyorange/type-definitions"
    ],
    "target": "ES2020",
    "module": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "allowJs": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}