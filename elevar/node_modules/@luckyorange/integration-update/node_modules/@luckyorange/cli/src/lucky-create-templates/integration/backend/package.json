{
  "name": "integrations-{{ name }}-backend",
  "version": "1.0.0",
  "description": "Processes backend system for {{ name }}",
  "main": "index.js",
  "watch": {
    "dev": "{src,test}/*.js"
  },
  "scripts": {
    "debug": "lucky start -- manifold run -- node --inspect-brk ./index.js",
    "debug:watch": "npm run debug -- --watch",
    "dev": "lucky start -- manifold run -- node ./index.js",
    "dev:watch": "npm-watch dev",
    "start": "manifold run -t $MANIFOLD_ENV -- node ./index.js",
    "test": "manifold run -t lucky-orange-staging -- jest",
    "watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luckyorange/integrations.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luckyorange/integrations/issues"
  },
  "homepage": "https://github.com/luckyorange/integrations#readme",
  "dependencies": {
    "@google-cloud/debug-agent": "^3.1.0",
    "@luckyorange/manifold-cli": "^0.15.1",
    "@luckyorange/utilities-integration-backend": "^1.0.6"
  },
  "devDependencies": {
    "@luckyorange/cli": "^3.8.2",
    "husky": "^1.3.1",
    "jest": "^23.6.0",    
    "nock": "^10.0.6",
    "npm-watch": "^0.6.0",
    "supertest": "^3.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lucky build"
    }
  }
}
