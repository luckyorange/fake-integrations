{
  "build": {
    "resources": [
      "readme",  
      "docker",
      {
        "name": "kubernetes",
        "options": {
          "namespace": "default",
          "redis": true,
          "auth": {
            "GET /oauth/authorize": "none",
            "GET /oauth/exchange": "none",
            "POST /webhooks/:name": "none",
            "GET /connect/ui": "none"
          }
        }
      },
      "jest-microservice"
    ]
  },
  "start": {
    "kubernetes": {
      "cluster": "api",
      "project": "lucky-orange-staging",
      "zone": "us-central1-c"
    }
  }
}
