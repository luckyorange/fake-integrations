{
  "name": "@luckyorange/secret-manager",
  "version": "0.1.1",
  "description": "Utility for managing secrets in a secure way. Will check for secrets on the machine, in a local cache, and in gcp secret manager.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "export": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "type-check": "tsc --noEmit",
    "type-check-dev": "tsc --noEmit --watch",
    "dev": "rollup -c -w",
    "lint": "biome lint src/",
    "format": "biome format --write src/",
    "format:check": "biome format src/",
    "check": "biome check src/",
    "test": "vitest",
    "test:debug": "vitest --silent false",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf dist node_modules coverage .turbo .rollup.cache tsconfig.tsbuildinfo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/secret-manager": "^5.6.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@vitest/coverage-v8": "^2.1.8",
    "rimraf": "^6.0.1",
    "rollup": "^4.28.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-bundle-stats": "^4.17.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "tslib": "^2.8.1",
    "turbo": "^2.3.3",
    "typescript": "5.7.2",
    "vitest": "^2.1.8"
  }
}
