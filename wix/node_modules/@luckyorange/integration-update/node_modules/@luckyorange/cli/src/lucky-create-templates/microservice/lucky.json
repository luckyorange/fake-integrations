{
  "build": {
    "resources": [
      "readme",
      { 
        "name" : "buddy",
        "options" : {
          "cluster" : { "name" : "{{cluster.name}}", "zone" : "{{cluster.zone}}" },
          "clusterStaging" : { "name" : "{{clusterStaging.name}}", "zone" : "{{clusterStaging.zone}}" }          
        }
      },      
      "docker",
      {
        "name": "kubernetes",
        "options": {
          "namespace": "{{namespace}}"
        }
      },
      "jest-microservice"
    ]
  },
  "start": {
    "kubernetes": {
      "cluster": "{{clusterStaging.name}}",
      "project": "lucky-orange-staging",
      "zone": "{{clusterStaging.zone}}"
    }
  }
}
